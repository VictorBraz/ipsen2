<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <TableView prefHeight="768.0" prefWidth="1024.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn maxWidth="1024.0" prefWidth="512.0" text="Client" />
                <TableColumn maxWidth="1024.0" minWidth="0.0" prefWidth="512.0" text="E-mail address" />
            </columns>
        </TableView>
    </center>
    <top>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER_LEFT">
            <buttons>
                <Button maxWidth="70.0" mnemonicParsing="false" prefWidth="70.0" text="Home" textOverrun="CLIP" />
                <Region prefHeight="200.0" prefWidth="900.0" />
                <Button maxWidth="70.0" mnemonicParsing="false" prefWidth="70.0" text="Edit" />
            </buttons>
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="20.0" top="10.0" />
            </BorderPane.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </ButtonBar>
    </top>
</BorderPane>
